NODE_ENV=local
PORT=5000
CHECK_INTERVAL=1000
TEMP_FILE_FOLDER=/tmp/aws_demo_app
AWS_REGION=ap-southeast-2
AWS_DEFAULT_REGION=ap-southeast-2
CORS_ORIGIN=http://localhost:3000
